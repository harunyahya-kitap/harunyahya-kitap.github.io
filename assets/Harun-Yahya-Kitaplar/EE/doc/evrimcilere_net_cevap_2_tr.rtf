\par \loch\af563\dbch\af13\hich\f563 \'d6\loch\f563 te yandan Kuran, Al\hich\af563\dbch\af13\loch\f563 lah'\loch\af563\dbch\af13\hich\f563 \u305\'69\hich\af563\dbch\af13\loch\f563 n tahrife u\loch\af563\dbch\af13\hich\f563 \u287\'67
